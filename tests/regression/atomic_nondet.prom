/**
 * Compile with -o1 in SPIN or with printf
 */

init {
    int a = 0; 
    int b = 0; 
    int c = 0;
    atomic {
        if
        :: a = 1
        :: a = 2
        fi;
        if
        :: b = 1
        :: b = 2
        fi;
        if
        :: c = 1
        :: c = 2
        fi
    };
    //printf ("a=%d b=%d, c=%d", a, b, c);
    //assert(false);
}

